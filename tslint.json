{
    "extends": "tslint:latest",
    "rules": {
        "indent": [
            true,
            "spaces",
            4
        ],
        "semicolon": true,
        "object-literal-sort-keys": false,
        "no-implicit-dependencies": false,
        "max-line-length": [
            true,
            300
        ],
        "trailing-comma": false,
        "arrow-parens": false
    }
}